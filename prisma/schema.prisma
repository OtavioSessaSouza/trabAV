// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
provider = "sqlite"
url = "file:./dev.db"
}

model Autores {
id Int @id @default(autoincrement())
nome String
nascimento String
biografia String
nacionalidade String
}

model Livros{
id Int @id @default(autoincrement())
titulo String
descricao String
dataPublicacao String
ISBN String @unique
preco Float
autorId Int
}
